@model ARID.Models.AccountViewModels.RoleModificationModel
<div class="bg-primary panel-body"><h4>Edit Role</h4></div>
<div asp-validation-summary="All" class="text-danger"></div>

<form asp-action="AddUser" method="post">
    <select asp-for="RoleName" class="form-control" asp-items="ViewBag.RoleAdminId"></select>
    <input asp-for="UserId" class="form-control"  />
    <button type="submit" class="btn btn-primary">Save</button>
    <a asp-action="Index" class="btn btn-default">Cancel</a>
</form>